
NEXT_PUBLIC_API_URL=http://localhost:3001/api
NEXT_PUBLIC_SERVER_URL=http://localhost:3001

# Supabase Project URL
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url

# Supabase Anon Key (public key, safe to expose)
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Google OAuth Client ID
google_client_id=your_google_client_id

# Google OAuth Client Secret 
google_secret=your_google_client_secret

# NextAuth Secret (required for JWT encryption)
NEXTAUTH_SECRET=your_nextauth_secret

# NextAuth URL (your app URL)
NEXTAUTH_URL=http://localhost:3000

NEXT_PUBLIC_SOLANA_NETWORK=devnet

# Solana RPC Endpoint (optional, defaults to public devnet RPC)
# For production, use a dedicated RPC provider like Helius, QuickNode, or Alchemy
NEXT_PUBLIC_RPC_ENDPOINT=https://api.devnet.solana.com

# Program ID for the Proven Stake program (required)
# This is your Solana program's public key
NEXT_PUBLIC_PROGRAM_ID=

NEXT_PUBLIC_USDC_MINT=

# Oracle Public Key for challenge validation (required)
NEXT_PUBLIC_ORACLE_PUBKEY=
